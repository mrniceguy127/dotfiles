Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


! URxvt
URxvt*font: xft:Hack:style=Regular:pixelsize=16:antialias=true:autohint=true
URxvt*scrollBar: False


! xst
st.font: Hack:style=Regular:pixelsize=16:antialias=true:autohint=true;
st.chscale: 0.8
st.cwscale: 0.8


! Colors

! special
*foreground:   #c0c5ce
*background:   #2b303b
*cursorColor:  #ffffff

! black
*color0:       #2b303b
*color8:       #65737e

! red
*color1:       #bf616a
*color9:       #bf616a

! green
*color2:       #a3be8c
*color10:      #a3be8c

! yellow
*color3:       #ebcb8b
*color11:      #ebcb8b

! blue
*color4:       #8fa1b3
*color12:      #8fa1b3

! magenta
*color5:       #b48ead
*color13:      #b48ead

! cyan
*color6:       #96b5b4
*color14:      #96b5b4

! white
*color7:       #c0c5ce
*color15:      #eff1f5
